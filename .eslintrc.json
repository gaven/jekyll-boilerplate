{
    "extends": "standard",
    "plugins": [
        "standard"
    ],
    "env": {
      "browser": true,
      "jquery": true
    },
    "rules": {
      "semi": ["error", "always"],
      "no-multi-spaces": [
        "error", {
          "exceptions": {
            "VariableDeclarator": true,
            "ImportDeclaration": true
          }
        }
      ]
    }
}
